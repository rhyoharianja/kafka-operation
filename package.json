{
	"name": "kafka-operation",
	"description": "Kafka operation extension for Directus Flows",
	"contributors": [
		{
			"name": "rhyoharianja",
			"url": "https://github.com/rhyoharianja"
		}
	],
	"license": "MIT",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"extension",
		"operation",
		"kafka"
	],
	"directus:extension": {
		"type": "operation",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"source": {
			"app": "src/app.ts",
			"api": "src/api.ts"
		},
		"host": "^10.0.0"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"unlink": "directus-extension unlink"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "10.0.0",
		"@types/node": "^20.0.0",
		"typescript": "^5.0.0"
	},
	"dependencies": {
		"axios": "^1.11.0",
		"kafkajs": "^2.2.4"
	}
}
